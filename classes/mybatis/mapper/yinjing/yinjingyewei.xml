<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--客户映射 指定到dao接口 -->
<mapper namespace="com.jcf.suchness.mapper.yinjing.YinjingYeweiMapper">
    <resultMap id="yeweiInfo" type="com.jcf.suchness.model.db.yinjing.YinjingYeweiInfo">
        <result property="collectTime" column="collectTime"/>
        <result property="unloadtime" column="unloadtime"/>
        <result property="value" column="value"/>
        <result property="terminalId" column="terminalId"/>
    </resultMap>

    <!--id与dao接口的方法名一直，指定结果类型，和参数类型 -->
    <select id="getInfoById" resultMap="yeweiInfo">
        SELECT top 1
            *
        FROM sqywdata WHERE terminalId = #{terminalId} ORDER BY collectTime DESC
    </select>
</mapper>